<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.xmf.job.admin.code.dao.CodeSchemeDao">

    <sql id="codeSchemeColumns">
	    str_to_date(a.createtime, '%Y-%m-%d %H:%i:%s') AS "createtimestr",
	   str_to_date(a.updatetime, '%Y-%m-%d %H:%i:%s') AS "updatetimestr",
		a.id AS "id",
		a.name AS "name",
		a.category AS "category",
		a.package_name AS "packageName",
		a.module_name AS "moduleName",
		a.sub_module_name AS "subModuleName",
		a.function_name AS "functionName",
		a.function_name_simple AS "functionNameSimple",
		a.function_author AS "functionAuthor",
		a.table_name AS "tableName",
		a.table_id AS "tableId",
		a.remark AS "remark",
		a.module_page_name AS "modulePageName",
		a.sub_page_name AS "subPageName"
	</sql>

    <sql id="codeSchemeJoins">
    </sql>

    <select id="getCodeSchemeById" resultType="com.cn.xmf.job.admin.code.model.CodeScheme">
        SELECT
        <include refid="codeSchemeColumns"/>
        FROM t_code_scheme a
        <include refid="codeSchemeJoins"/>
        WHERE a.id = #{id}
    </select>

    <select id="getTotalCount" resultType="Integer" parameterType="com.alibaba.fastjson.JSONObject">
        SELECT
        count(*) as count
        FROM t_code_scheme a
        <include refid="codeSchemeJoins"/>
        <where>
            1=1
            <if test="name != null and name != ''">
                AND a.name = #{name}
            </if>
            <if test="category != null and category != ''">
                AND a.category = #{category}
            </if>
            <if test="tableName != null and tableName != ''">
                AND a.table_name = #{tableName}
            </if>
            <if test="tableId != null and tableId != ''">
                AND a.table_id = #{tableId}
            </if>

        </where>
    </select>

    <select id="getList" resultType="com.cn.xmf.job.admin.code.model.CodeScheme"
            parameterType="com.alibaba.fastjson.JSONObject">
        SELECT
        <include refid="codeSchemeColumns"/>
        FROM
        (SELECT id from t_code_scheme t
        <where>
            1=1
            <if test="name != null and name != ''">
                AND t.name = #{name}
            </if>
            <if test="category != null and category != ''">
                AND t.category = #{category}
            </if>
            <if test="tableName != null and tableName != ''">
                AND t.table_name = #{tableName}
            </if>
            <if test="tableId != null and tableId != ''">
                AND t.table_id = #{tableId}
            </if>
        </where>
        limit #{startIndex},#{pageSize}) b inner join
        t_code_scheme a on b.id=a.id
        <choose>
            <otherwise>
                ORDER BY createtime desc
            </otherwise>
        </choose>
    </select>

    <select id="getCodeSchemeList" resultType="com.cn.xmf.job.admin.code.model.CodeScheme"
            parameterType="com.cn.xmf.job.admin.code.model.CodeScheme">
        SELECT
        <include refid="codeSchemeColumns"/>
        FROM (SELECT id from t_code_scheme t
        <where>
            1=1
            <if test="name != null and name != ''">
                AND t.name = #{name}
            </if>
            <if test="category != null and category != ''">
                AND t.category = #{category}
            </if>
            <if test="tableName != null and tableName != ''">
                AND t.table_name = #{tableName}
            </if>
            <if test="tableId != null and tableId != ''">
                AND t.table_id = #{tableId}
            </if>
        </where>
        ) b inner join t_code_scheme a on b.id=a.id
        <choose>
            <otherwise>
                ORDER BY createtime desc
            </otherwise>
        </choose>
    </select>

    <select id="getSignleCodeScheme" resultType="com.cn.xmf.job.admin.code.model.CodeScheme"
            parameterType="com.cn.xmf.job.admin.code.model.CodeScheme">
        SELECT
        <include refid="codeSchemeColumns"/>
        FROM
        t_code_scheme a
        <where>
            1=1
            <if test="name != null and name != ''">
                AND a.name = #{name}
            </if>
            <if test="category != null and category != ''">
                AND a.category = #{category}
            </if>
            <if test="tableName != null and tableName != ''">
                AND a.table_name = #{tableName}
            </if>
            <if test="tableId != null and tableId != ''">
                AND a.table_id = #{tableId}
            </if>
        </where>
        limit 1
    </select>


    <insert id="add" useGeneratedKeys="true" keyProperty="id">
